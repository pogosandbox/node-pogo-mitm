{
    // Use IntelliSense to learn about possible Node.js debug attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "protocol": "auto",
            "program": "${workspaceRoot}/src/index.ts",
            "cwd": "${workspaceRoot}",
            "outFiles": [ "${workspaceRoot}/bin/**/*.js" ],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Preload",
            "protocol": "auto",
            "program": "${workspaceRoot}/src/utils/preload.ts",
            "cwd": "${workspaceRoot}",
            "outFiles": [ "${workspaceRoot}/bin/**/*.js" ],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Import from iOS dump",
            "protocol": "auto",
            "program": "${workspaceRoot}/src/import/ios.dump.ts",
            "cwd": "${workspaceRoot}",
            "outFiles": [ "${workspaceRoot}/bin/**/*.js" ],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Import from Noctem iOS dump",
            "protocol": "auto",
            "program": "${workspaceRoot}/src/import/ios.dump.noctem.ts",
            "cwd": "${workspaceRoot}",
            "outFiles": [ "${workspaceRoot}/bin/**/*.js" ],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Import Snorlax",
            "protocol": "auto",
            "program": "${workspaceRoot}/src/import/snorlax.ts",
            "cwd": "${workspaceRoot}",
            "outFiles": [ "${workspaceRoot}/bin/**/*.js" ],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "CSV Export",
            "protocol": "auto",
            "program": "${workspaceRoot}/src/export/csv.ts",
            "cwd": "${workspaceRoot}",
            "outFiles": [ "${workspaceRoot}/bin/**/*.js" ],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "debug.js",
            "protocol": "auto",
            "program": "${workspaceRoot}/debug.js",
            "cwd": "${workspaceRoot}"
        }
    ]
}