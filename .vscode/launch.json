{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceRoot}/src/index.ts",
            "cwd": "${workspaceRoot}",
            "outFiles": [ "${workspaceRoot}/bin/**/*.js" ],
            "sourceMaps": true,
            "stopOnEntry": false
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Analyse",
            "program": "${workspaceRoot}/src/utils/analysis.ts",
            "args": [
                "20170902.160717"                
            ],
            "cwd": "${workspaceRoot}",
            "outFiles": [ "${workspaceRoot}/bin/**/*.js" ],
            "sourceMaps": true,
            "stopOnEntry": false
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Import from iOS dump",
            "program": "${workspaceRoot}/src/import/ios.dump.ts",
            "cwd": "${workspaceRoot}",
            "outFiles": [ "${workspaceRoot}/bin/**/*.js" ],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Import Snorlax",
            "program": "${workspaceRoot}/src/import/snorlax.ts",
            "cwd": "${workspaceRoot}",
            "outFiles": [ "${workspaceRoot}/bin/**/*.js" ],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Import Fiddler",
            "program": "${workspaceRoot}/src/import/fiddler.ts",
            "cwd": "${workspaceRoot}",
            "outFiles": [ "${workspaceRoot}/bin/**/*.js" ],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "CSV Export",
            "program": "${workspaceRoot}/src/export/csv.ts",
            "cwd": "${workspaceRoot}",
            "outFiles": [ "${workspaceRoot}/bin/**/*.js" ],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "quick",
            "program": "${workspaceRoot}/src/utils/quick.ts",
            "cwd": "${workspaceRoot}",
            "outFiles": [ "${workspaceRoot}/bin/**/*.js" ],
            "sourceMaps": true
        }
    ]
}